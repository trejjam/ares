{
	"name": "trejjam/ares",
	"description": "Ares integration into Nette framework",
	"keywords": [
		"nette",
		"ares",
		"ico"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Trejbal",
			"homepage": "https://github.com/trejjam"
		}
	],
	"require": {
		"php": "~8.1.0 | ~8.2.0",

		"ext-simplexml": "~8.1.0 | ~8.2.0",

		"nette/di": "^3.0",
		"nette/http": "^3.0",
		"nette/utils": "^3.0",
		"guzzlehttp/guzzle": "^7.0"
	},
	"require-dev": {
		"nette/tester": "^2.4.3",

		"composer/ca-bundle": "^1.3.5",
		"paragonie/certainty": "^2.8.2",

		"phpstan/phpstan": "^1.9.14",
		"phpstan/extension-installer": "^1.2.0",
		"phpstan/phpstan-deprecation-rules": "^1.1.1",
		"phpstan/phpstan-strict-rules": "^1.4.5",
		"roave/no-floaters": "^1.9.0",

		"symplify/easy-coding-standard": "^11.2.5",

		"roave/security-advisories": "dev-master"
	},
	"suggest": {
		"composer/ca-bundle": "Static CA root chain provider",
		"paragonie/certainty": "Keep your CA root chain up to date"
	},
	"autoload": {
		"psr-4": {
			"Trejjam\\Ares\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Trejjam\\Ares\\Tests\\": "tests/src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "v0.7.x-dev"
		}
	}
}
